							.input__text Гражданство
							.input__hint
								ul.input__hint__list
									li.input__hint__elem
										a(href="#").input__hint__link
											.input__hint__name Россия
									li.input__hint__elem
										a(href="#").input__hint__link
											.input__hint__name Украина
									li.input__hint__elem
										a(href="#").input__hint__link
											.input__hint__name Беларусь
									li.input__hint__elem
										a(href="#").input__hint__link
											.input__hint__name Румыния
									li.input__hint__elem
